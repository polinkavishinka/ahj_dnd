{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "import/no-cycle": "off",
        "no-restricted-globals": "off",
        "no-empty": "off",
        "nno-shadow": "off",
        "no-restricted-syntax": "off"
        
    }
}